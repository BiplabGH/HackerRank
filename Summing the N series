int main() {
   int t;
    scanf("%d",&t);
        for(int i=0;i<t;i++)
        {
            unsigned long long n;
            scanf("%lld",&n);
            //if(n>=1 && n<=10000000000000000)
            //{
             unsigned long long tvalue=1000000000+7;

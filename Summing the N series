            j=n % tvalue;
            unsigned long long k=n % tvalue;
           k=j*k;
           j=k%tvalue;
            printf("%lld\n",j);
            //}
            //Console.WriteLine("\n");
        }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
